  /usr/share/doc/libfpx-1.2.0.13/AUTHORS
  /usr/share/doc/libfpx-1.2.0.13/COPYING
  /usr/share/doc/libfpx-1.2.0.13/ChangeLog
  /usr/share/doc/libfpx-1.2.0.13/INSTALL
  /usr/share/doc/libfpx-1.2.0.13/NEWS
  /usr/share/doc/libfpx-1.2.0.13/README
  /usr/share/doc/libfpx-1.2.0.13/fpx_api.pdf
  /usr/share/doc/libfpx-1.2.0.13/readme.txt
  /usr/share/doc/libfpx-1.2.0.13/ri_arch.pdf

(libfpx1)
  /usr/bin/cygfpx-1.dll

